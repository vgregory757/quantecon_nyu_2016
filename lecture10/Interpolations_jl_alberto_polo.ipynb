    "# utility function defined based on cash-on-hand\n",
    "function u(aprime, holder, holder_ce)\n",
    "    risk_av = holder.risk_av\n",
    "    cash = holder_ce.cash\n",
    "    (cash - aprime).^(1-risk_av)/(1-risk_av)\n",
    "end"
   ]
